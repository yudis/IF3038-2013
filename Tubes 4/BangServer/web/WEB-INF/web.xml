<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <listener>
        <listener-class>com.sun.xml.ws.transport.http.servlet.WSServletContextListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>UserSoap</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserSoap</servlet-name>
        <url-pattern>/UserSoap</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CategorySoap</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>CategorySoap</servlet-name>
        <url-pattern>/CategorySoap</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>TaskSoap</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>TaskSoap</servlet-name>
        <url-pattern>/TaskSoap</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CommentSoap</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>CommentSoap</servlet-name>
        <url-pattern>/CommentSoap</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserRest</servlet-name>
        <servlet-class>REST.UserRest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getSearchData</servlet-name>
        <servlet-class>REST.getSearchData</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserRest</servlet-name>
        <url-pattern>/user/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getSearchData</servlet-name>
        <url-pattern>/header/*</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Bang</servlet-name>
        <servlet-class>REST.Bang</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Bang</servlet-name>
        <url-pattern>/Bang/*</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
