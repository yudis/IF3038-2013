<web-app id="WebApp_ID" version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>cobaREST</display-name>
    <servlet>
        <servlet-name>AnswererSelect</servlet-name>
        <servlet-class>answerer.AnswererSelect</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AnswererNonSelectREST</servlet-name>
        <servlet-class>answerer.AnswererNonSelectREST</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AnswererSelect</servlet-name>
        <url-pattern>/query-select/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AnswererNonSelectREST</servlet-name>
        <url-pattern>/query-nonselect-rest/*</url-pattern>
    </servlet-mapping>
